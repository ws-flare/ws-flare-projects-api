{
  "name": "ws-flare-projects-api",
  "version": "1.0.0",
  "description": "ws-flare-projects-api",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha -t 120000 --allow-console-logs --exit \"dist/__tests__\"",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.0.14",
    "@loopback/context": "^1.6.0",
    "@loopback/core": "^1.1.7",
    "@loopback/openapi-v3": "^1.2.3",
    "@loopback/repository": "^1.1.7",
    "@loopback/rest": "^1.7.0",
    "@loopback/rest-explorer": "^1.1.10",
    "@loopback/service-proxy": "^1.0.9",
    "loopback-connector-couchdb2": "^1.3.0",
    "loopback-connector-mysql": "^5.3.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^1.3.1",
    "@loopback/testlab": "^1.0.7",
    "@loopback/tslint-config": "^2.0.1",
    "@types/async": "^2.4.1",
    "@types/async-retry": "^1.2.1",
    "@types/dockerode": "^2.5.12",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.11.2",
    "@types/uuid": "^3.4.4",
    "async": "^2.6.2",
    "async-retry": "^1.2.3",
    "mock-couch": "^0.1.11",
    "mysql": "^2.16.0",
    "node-docker-api": "^1.1.22",
    "promise-mysql": "^3.3.1",
    "random-port": "^0.1.0",
    "random-port-as-promised": "^1.0.0",
    "testcontainers": "^1.1.10",
    "tslint": "^5.12.0",
    "typescript": "^3.3.1"
  }
}
